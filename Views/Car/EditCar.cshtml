@model Car



<div class="product-INfo">
    <div class="title-container">
        <h2 class="product-title">Car Info</h2>
    </div>
    <div class="flex-container">
        <div class="product-image">
            <img src="@Model.ImageUrl" alt="Product Image">
        </div>
        <div class="product-info">
            <p><span class="strong-text">Model:</span> @Model.Name</p>
            <p><span class="strong-text">Vendor:</span> @Model.Vendor</p>
            <p><span class="strong-text">Price:</span> <span class="price-text">@Model.Price $</span></p>
        </div>
    </div>
</div>